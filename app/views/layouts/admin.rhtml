<html>
<head>
  <%= javascript_include_tag 'prototype'%>
  <%= javascript_include_tag 'effects'%>
  <%= javascript_include_tag 'dragdrop'%>
  <%= javascript_include_tag 'controls'%>
  
  <%= javascript_include_tag "/extjs/adapter/prototype/ext-prototype-adapter.js"%>
  <%= javascript_include_tag "/extjs/ext-all.js" %>

  <%= javascript_include_tag 'admin/admin.js' %>

  <%= stylesheet_link_tag    "/extjs/resources/css/ext-all.css" %>
    
  <%= yield :header %>
</head>  
<body>
  
  <div id="sidebar"><%= yield :sidebar %></div>
  <div id="content"><%= yield :content %></div>
  
  <script type="text/javascript">
    var controllerInfo = <%= @controller_info.to_json %>;
  </script>

  <%= yield :javascript %>
</body>
</html>